{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "twitter-bootstrap@4.5.0",
      "destination": "wwwroot/bootstrap/"
    },
    {
      "library": "jquery@3.5.1",
      "destination": "wwwroot/jquery/"
    },
    {
      "library": "jquery-validate@1.19.2",
      "destination": "wwwroot/jquery/jquery-validate/",
      "files": [
        "jquery.validate.min.js",
        "additional-methods.js",
        "additional-methods.min.js",
        "jquery.validate.js"
      ]
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.11",
      "destination": "wwwroot/jquery/jquery-validation-unobtrusive/"
    },
    {
      "library": "font-awesome@5.12.0-2",
      "destination": "wwwroot/bootstrap/fonts/font-awesome/",
      "files": [
        "css/all.min.css",
        "js/all.min.js",
        "sprites/brands.svg",
        "sprites/regular.svg",
        "sprites/solid.svg",
        "webfonts/fa-brands-400.eot",
        "webfonts/fa-brands-400.svg",
        "webfonts/fa-brands-400.ttf",
        "webfonts/fa-brands-400.woff",
        "webfonts/fa-brands-400.woff2",
        "webfonts/fa-regular-400.eot",
        "webfonts/fa-regular-400.svg",
        "webfonts/fa-regular-400.ttf",
        "webfonts/fa-regular-400.woff",
        "webfonts/fa-regular-400.woff2",
        "webfonts/fa-solid-900.eot",
        "webfonts/fa-solid-900.svg",
        "webfonts/fa-solid-900.ttf",
        "webfonts/fa-solid-900.woff",
        "webfonts/fa-solid-900.woff2"
      ]
    },
    {
      "library": "datatables.net-responsive@2.2.5",
      "destination": "wwwroot/datatables/js/",
      "files": [
        "dataTables.responsive.min.js"
      ]
    },
    {
      "library": "datatables@1.10.20",
      "destination": "wwwroot/datatables/",
      "files": [
        "css/dataTables.bootstrap4.min.css"
      ]
    }
  ]
}